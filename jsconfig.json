{
  "compilerOptions": {
    "target": "ESNext",
    "module": "System",
    "moduleResolution": "Node",
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "checkJs": true,
    "baseUrl": ".",
    "paths": {
      "dist/*": ["./dist/*"]
    }
  },
  "exclude": [".git", "node_modules"]
}